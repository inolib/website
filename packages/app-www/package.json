{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@inolib/app-www",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run build:app && pnpm run build:server && pnpm run build:sitemap",
    "build:app": "npm run generate-api && next build",
    "build:app:ci": "next build",
    "build:server": "tsup",
    "build:sitemap": "dotenvx run -- next-sitemap",
    "dev": "next dev",
    "format": "pnpm run format:prettier && pnpm run format:eslint",
    "format:eslint": "eslint --fix --format silent .",
    "format:prettier": "prettier --log-level warn --write .",
    "generate-api": "npx @openapitools/openapi-generator-cli generate   -i ../app-strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json   -g typescript-axios -o src/lib/strapi/api-client   --skip-validate-spec --additional-properties=useSingleRequestParameter=true,supportsES6=true > /dev/null 2>&1",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc",
    "serve:development": "dotenvx run -f ./.env.development -- tsx ./src/server.ts",
    "serve:production": "dotenvx run -- node ./dist/server.cjs"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.39.0",
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@emotion/styled": "^11.14.0",
    "@fastify/compress": "^8.0.1",
    "@fontsource-variable/manrope": "^5.1.0",
    "@mui/material": "^6.4.2",
    "@tanstack/react-form": "^0.39.0",
    "@tanstack/valibot-form-adapter": "^0.39.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "fastify": "^5.1.0",
    "framer-motion": "^12.0.11",
    "next": "^15.0.3",
    "orejime": "^2.3.1",
    "react": "^18.3.1",
    "react-aria": "^3.37.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "1.0.0-beta.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@dotenvx/dotenvx": "^1.28.0",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-perfectionist": "^4.2.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.4.16",
    "ts-loader": "^9.5.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
